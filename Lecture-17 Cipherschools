#include <iostream>
using namespace std;
void PrintHi()
{
cout<<"Hi"<<endl;
return;
}

int sum(int a, int b)
{
	int c;
	c= a+b;
	a=500;
	return c;
}

int main()
{
	PrintHi();
	
	int a,b;
	 a=4;
	 b=6;
	
	c=75;
	 int d;
	 d =sum(a,b);
	 
	 cout<<"The sum is equal to "<<d<<" "<<a;
	 cout<<"The value of c is"<<c;
	return 0;
}
